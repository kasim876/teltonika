mixin head(data)
  -
    const title = data.title

  meta(charset="UTF-8")
  meta(http-equiv="X-UA-Compatible", content="IE=edge")
  meta(name="viewport", content="width=device-width, initial-scale=1.0")

  title= title

  link(rel="preload", href="resources/fonts/Raleway-Bold.woff2", as="font", type="font/woff2", crossorigin)
  link(rel="preload", href="resources/fonts/Raleway-SemiBold.woff2", as="font", type="font/woff2", crossorigin)
  link(rel="preload", href="resources/fonts/Raleway-Medium.woff2", as="font", type="font/woff2", crossorigin)
  link(rel="preload", href="resources/fonts/Raleway-Regular.woff2", as="font", type="font/woff2", crossorigin)